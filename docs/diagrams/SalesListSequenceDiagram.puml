@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant "s:SalesListCommand" as SalesListCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR

end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute(salesList)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(salesList)
activate AddressBookParser

create SalesListCommand
AddressBookParser -> SalesListCommand : SalesListCommand(sales)
activate SalesListCommand

SalesListCommand --> AddressBookParser : s
deactivate SalesListCommand

AddressBookParser --> LogicManager : s
deactivate AddressBookParser

LogicManager -> SalesListCommand : execute()
activate SalesListCommand

SalesListCommand -> Model : sortSalesBook()
activate Model

Model --> SalesListCommand
deactivate Model

SalesListCommand -> Model : updateFilteredSalesRecordList(Predicate)
activate Model

Model --> SalesListCommand
deactivate Model

create CommandResult
SalesListCommand -> CommandResult : CommandResult(message)
activate CommandResult

CommandResult --> SalesListCommand
deactivate CommandResult

SalesListCommand --> LogicManager : result
deactivate SalesListCommand

[<--LogicManager
deactivate LogicManager

@enduml